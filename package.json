{
	"name": "svelte-starter-kit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:node": "cross-env ADAPTER=node npm run build",
		"build:static": "cross-env ADAPTER=static npm run build",
		"preview": "vite preview",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@floating-ui/dom": "1.0.1",
		"@playwright-testing-library/test": "4.3.1",
		"@playwright/test": "1.25.2",
		"@popperjs/core": "2.11.6",
		"@sveltejs/adapter-auto": "1.0.0-next.72",
		"@sveltejs/adapter-node": "1.0.0-next.89",
		"@sveltejs/adapter-static": "1.0.0-next.42",
		"@sveltejs/kit": "1.0.0-next.480",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/line-clamp": "0.4.2",
		"@tailwindcss/typography": "0.5.7",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/svelte": "3.2.1",
		"@types/testing-library__jest-dom": "5.14.5",
		"@typescript-eslint/eslint-plugin": "5.36.2",
		"@typescript-eslint/parser": "5.36.2",
		"@vitest/coverage-c8": "0.23.2",
		"autoprefixer": "10.4.9",
		"c8": "7.12.0",
		"classnames": "2.3.1",
		"cross-env": "7.0.3",
		"cross-fetch": "3.1.5",
		"cssnano": "5.1.13",
		"eslint": "8.23.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"flowbite": "1.5.3",
		"flowbite-svelte": "0.26.17",
		"jsdom": "20.0.0",
		"msw": "0.47.1",
		"postcss": "8.4.16",
		"postcss-load-config": "4.0.1",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "2.7.0",
		"svelte": "3.50.1",
		"svelte-check": "2.9.0",
		"svelte-preprocess": "4.10.7",
		"tailwindcss": "3.1.8",
		"tslib": "2.4.0",
		"typescript": "4.8.3",
		"vite": "3.1.0",
		"vitest": "0.23.2"
	},
	"type": "module",
	"dependencies": {
		"cookie": "0.5.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/xmlking/svelte-starter-kit"
	}
}
